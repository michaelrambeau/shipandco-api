{
  "name": "shipandco-api",
  "description": "Shipandco API built with Feathers",
  "version": "0.1.0",
  "main": "src/shipandco-api.js",
  "keywords": [
    "feathers"
  ],
  "license": "MIT",
  "repository": {},
  "author": "Michael Rambeau",
  "engines": {
    "node": ">= 6.2.0"
  },
  "scripts": {
    "start": "node ./src/shipandco-api.js",
    "production": "DB_ENV=production forever start ./src/shipandco-api.js -l forever-log.txt",
    "stop": "forever stopall",
    "list": "forever list",
    "dev": "DEBUG=* nodemon ./src/shipandco-api.js",
    "deploy": "now -e NODE_ENV=production -e MONGO_URL=@shipandco_dev_mongo_url -e AUTH0_SECRET=@shipandco_auth0_secret -e AUTH0_ID=@shipandco_auth0_id -e TOKEN_SECRET=@shipandco_token_secret",
    "deploy:prod": "now -e NODE_ENV=production -e DB_ENV=production -e MONGO_URL_PRODUCTION=@shipandco_prod_mongo_url -e MONGO_URL_ADMIN_USERS=@shipandco_admin_mongo_url -e AUTH0_SECRET=@shipandco_auth0_secret -e AUTH0_ID=@shipandco_auth0_id -e TOKEN_SECRET=@shipandco_token_secret"
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "dotenv": "^2.0.0",
    "feathers": "2.1.2",
    "feathers-authentication": "^0.7.1",
    "feathers-errors": "^2.0.1",
    "feathers-hooks": "1.6.1",
    "feathers-mongoose": "5.1.0",
    "feathers-rest": "1.7.2",
    "forever": "^0.15.3",
    "fs-extra": "^4.0.0",
    "lodash.get": "^4.4.2",
    "mongoose": "4.10.4",
    "passport-auth0": "^0.5.1",
    "stripe": "^4.23.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "3.14.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-promise": "^2.0.1",
    "prettier": "^1.5.3"
  }
}
